{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "require-jsdoc": "off",
        "react/react-in-jsx-scope": "off",
        "max-len": ["error", { "code": 150, "ignoreStrings": true, "ignoreComments": true }]
    }
}
